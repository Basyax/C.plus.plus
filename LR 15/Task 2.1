#include <iostream>            // Підключення бібліотеки введення-виведення
using namespace std;           // Простір імен std

// Функція перевірки високосного року
bool isLeap(int year) {
    return (year % 4 == 0 && year % 100 != 0) || (year % 400 == 0);
}

// Функція, що повертає кількість днів у конкретному місяці певного року
int monthLength(int year, int month) {
    switch(month) {            // Вибір дії залежно від номера місяця
        case 2:                // Якщо лютий
            return isLeap(year) ? 29 : 28; // Якщо високосний — 29, інакше — 28
        case 4: case 6: case 9: case 11:   // Місяці з 30 днями
            return 30;
        default:               // Всі інші місяці мають 31 день
            return 31;
    }
}

int main() {
    for (int yr = 2000; yr < 2002; yr++) {     // Перебираємо роки 2000 та 2001
        for (int mo = 1; mo <= 12; mo++) {     // Перебираємо всі 12 місяців
            cout << monthLength(yr, mo) << " "; // Виводимо кількість днів
        }
        cout << endl;                          // Перехід на новий рядок після кожного року
    }
    return 0;
}
