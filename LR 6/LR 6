#include <iostream> // Для вводу/виводу
using namespace std;

int main() {
    int N; // Змінна для введення натурального числа
    cout << "Введіть натуральне число N: "; // Запит на введення числа
    cin >> N; // Зчитуємо число з клавіатури

    int count = 0; // Лічильник цифр, які не дорівнюють 1 і 9
    int digit;     // Змінна для зберігання поточної цифри числа

    while (N > 0) { // Цикл працює, поки N не стане 0
        digit = N % 10; // Відділяємо останню цифру числа
        if (digit != 1 && digit != 9) { // Перевіряємо, чи цифра не дорівнює 1 або 9
            count++; // Збільшуємо лічильник
        }
        N /= 10; // Видаляємо останню цифру числа
    }

    // Виводимо результат
    cout << "Кількість цифр, що не дорівнюють 1 і 9: " << count << endl;

    return 0; // Завершення програми
}
